@techreport{AMVargas2017,
abstract = {Drones are being used in an increasing number of civilian applications. The quadcopter is a type of drone which has undergone extensive research making it an excellent testbed for novel control techniques. Several intended uses of quadcopters require operation in confined environments, in which objects are in close proximity to the vehicle. In these conditions, the flight is affected by aerodynamic interactions (force and torque). Intuitively, these in-teractions can be viewed as air flow rebounding on the surroundings back to the vehicle. The development of efficient computational approaches for describing such interactions re-mains open to improvement as existing accurate models require a considerable amount of computational load and cannot be used in the real-time control loop of the quadrotor. This research hypothesizes that with a simplified mathematical model, which can be deployed in real-time and approximates the behavior of the aerodynamic interactions, the flight control of the quadrotor is improved. In seeking to confirm this hypothesis, the aim is to develop an efficient model of aerodynamic interactions which can be retrieved from simulated and experimental data. Three major areas of knowledge shall be explored to solve the problem: control theory, artificial intelligence, and fluid mechanics. As a preliminary progress, numer-ical optimization techniques for nonlinear quadrotor control are proposed.},
address = {Puebla},
author = {by {Antonio Matus Vargas} and {Rodr{\'{i}}guez G{\'{o}}mez Jos{\'{e}} Mart{\'{i}}nez Carranza}, Gustavo},
file = {:Users/josedenaruiz/Library/Application Support/Mendeley Desktop/Downloaded/Vargas - 2017 - Quadrotor Flight in Constrained Indoor Environments.pdf:pdf},
institution = {Instituto Nacional de Astrofısica, Optica y Electronica},
pages = {63},
title = {{Quadrotor Flight in Constrained Indoor Environments}},
url = {https://ccc.inaoep.mx/archivos/CCC-17-005.pdf http://ccc.inaoep.mx/archivos/CCC-17-005.pdf},
year = {2017}
}
